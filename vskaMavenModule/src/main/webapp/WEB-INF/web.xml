<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <servlet>
        <servlet-name>ObjectTypeCreateServlet</servlet-name>
        <servlet-class>vska.servlet.objecttype.ObjectTypeCreateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectTypeCreateServlet</servlet-name>
        <url-pattern>/createobjecttype</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectTypeDelete</servlet-name>
        <servlet-class>vska.servlet.objecttype.ObjectTypeDelete</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectTypeDelete</servlet-name>
        <url-pattern>/deleteobjecttype</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectTypeList</servlet-name>
        <servlet-class>vska.servlet.objecttype.ObjectTypeListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectTypeList</servlet-name>
        <url-pattern>/objecttypelist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectCreateServlet</servlet-name>
        <servlet-class>vska.servlet.object.ObjectCreateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectCreateServlet</servlet-name>
        <url-pattern>/createobject</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>DeleteObjectServlet</servlet-name>
