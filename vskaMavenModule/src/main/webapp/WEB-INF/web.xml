    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteObjectServlet</servlet-name>
        <url-pattern>/deleteobject</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectListServlet</servlet-name>
        <servlet-class>vska.servlet.object.ObjectListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectListServlet</servlet-name>
        <url-pattern>/objectlist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AttributeCreateServlet</servlet-name>
        <servlet-class>vska.servlet.attribute.AttributeCreateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AttributeCreateServlet</servlet-name>
        <url-pattern>/createattribute</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AttributeDeleteServlet</servlet-name>
        <servlet-class>vska.servlet.attribute.AttributeDeleteServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AttributeDeleteServlet</servlet-name>
        <url-pattern>/deleteattribute</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AttributeListServlet</servlet-name>
        <servlet-class>vska.servlet.attribute.AttributeListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AttributeListServlet</servlet-name>
        <url-pattern>/attributelist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectSearchServlet</servlet-name>
        <servlet-class>vska.servlet.object.ObjectSearchServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectSearchServlet</servlet-name>
        <url-pattern>/resofobjectsbysearch</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ObjectParametersServlet</servlet-name>
        <servlet-class>vska.servlet.object.ObjectParametersServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ObjectParametersServlet</servlet-name>
        <url-pattern>/objectparameters</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AttributeUpdateServlet</servlet-name>
        <servlet-class>vska.servlet.attribute.AttributeUpdateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AttributeUpdateServlet</servlet-name>
